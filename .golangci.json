{
	"run": {
		"tests": false
	},
	"linters": {
		"enable": [
			"bodyclose",
			"deadcode",
			"depguard",
			"dogsled",
			"dupl",
			"errcheck",
			"exportloopref",
			"exhaustive",
			"funlen",
			"gocognit",
			"gocritic",
			"gocyclo",
			"golint",
			"goprintffuncname",
			"gosimple",
			"govet",
			"ineffassign",
			"lll",
			"misspell",
			"nakedret",
			"nestif",
			"noctx",
			"nolintlint",
			"rowserrcheck",
			"staticcheck",
			"structcheck",
			"stylecheck",
			"testpackage",
			"typecheck",
			"unconvert",
			"unparam",
			"unused",
			"varcheck",
			"whitespace"
		]
	},
	"linters-settings": {
		"funlen": {},
		"gocritic": {
			"disabled-checks": ["commentFormatting"]
		},
		"gomnd": {
			"settings": {
				"mnd": {
					"ignored-numbers": [100, 1000]
				}
			}
		}
	}
}
